# Notion API credentials
NOTION_API_KEY=your_notion_api_key_here

# Notion Database IDs
NOTION_TRAITS_DB_ID=your_traits_database_id
NOTION_SPELLS_DB_ID=your_spells_database_id

# For ancestries and archetypes stored as Notion pages rather than in databases
# Comma-separated list of Notion page IDs
NOTION_ANCESTRY_PAGES=page_id_1,page_id_2,page_id_3
NOTION_ARCHETYPE_PAGES=page_id_1,page_id_2,page_id_3

# Path to custom JSON file with ancestry/archetype data (optional)
# CUSTOM_JSON_PATH=./data/custom-ancestries.json

# Vite environment variables (for browser usage)
VITE_NOTION_API_KEY=${NOTION_API_KEY}
VITE_NOTION_TRAITS_DB_ID=${NOTION_TRAITS_DB_ID}
VITE_NOTION_SPELLS_DB_ID=${NOTION_SPELLS_DB_ID} 